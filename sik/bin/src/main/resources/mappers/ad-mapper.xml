<mapper namespace="adMapper">

	<resultMap id="resultAd" type="Ad">
		<id property="adId" column="AD_ID" />
		<result property="adTitle" column="AD_TITLE" />
		<result property="adBir" column="AD_BIR" />
		<result property="adStDate" column="AD_ST_DATE" />
		<result property="adEndDate" column="AD_END_DATE" />
		<result property="adStatus" column="AD_STATUS" />
